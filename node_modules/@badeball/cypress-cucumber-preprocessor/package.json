{
  "name": "@badeball/cypress-cucumber-preprocessor",
  "version": "13.0.2",
  "author": "Jonas Amundsen",
  "license": "MIT",
  "homepage": "https://github.com/badeball/cypress-cucumber-preprocessor",
  "repository": {
    "type": "git",
    "url": "https://github.com/badeball/cypress-cucumber-preprocessor.git"
  },
  "keywords": [
    "cypress",
    "cypress-plugin",
    "cypress-preprocessor"
  ],
  "bin": {
    "cypress-cucumber-diagnostics": "lib/diagnostics/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": "methods.js",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "browserify.js",
    "browserify.d.ts",
    "esbuild.js",
    "esbuild.d.ts",
    "methods.js",
    "methods.d.ts",
    "webpack.js",
    "webpack.d.ts"
  ],
  "scripts": {
    "clean": "rm -f {browserify,esbuild,methods,webpack}.{js,d.ts} && bash -O globstar -c 'rm -f lib/**/*.{js,d.ts} test/**/*.{js,d.ts}'",
    "build": "tsc",
    "watch": "tsc --watch",
    "fmt": "prettier --ignore-path .gitignore --write '**/*.ts'",
    "test": "npm run test:fmt && npm run test:types && npm run test:unit && npm run test:integration",
    "test:fmt": "prettier --ignore-path .gitignore --check '**/*.ts'",
    "test:types": "tsd",
    "test:unit": "mocha lib/**/*.test.ts",
    "test:run-all-specs": "mocha --timeout 0 test/run-all-specs.ts",
    "test:integration": "cucumber-js",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "dependencies": {
    "@badeball/cypress-configuration": "^4.0.0",
    "@cucumber/cucumber-expressions": "^16.0.0",
    "@cucumber/gherkin": "^24.0.0",
    "@cucumber/html-formatter": "^19.2.0",
    "@cucumber/message-streams": "^4.0.1",
    "@cucumber/messages": "^19.1.2",
    "@cucumber/tag-expressions": "^4.1.0",
    "base64-js": "^1.5.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.2",
    "common-ancestor-path": "^1.0.1",
    "cosmiconfig": "^7.0.1",
    "debug": "^4.2.0",
    "error-stack-parser": "^2.0.7",
    "esbuild": "^0.14.23",
    "glob": "^7.2.0",
    "is-path-inside": "^3.0.3",
    "module-alias": "^2.2.2",
    "node-hook": "^1.0.0",
    "resolve-pkg": "^2.0.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.2",
    "@cucumber/cucumber": "^8.0.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@types/common-ancestor-path": "^1.0.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node-hook": "^1.0.1",
    "@types/prettier": "^2.6.3",
    "@types/stream-buffers": "^3.0.4",
    "ast-types": "^0.15.2",
    "cypress": "^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "mocha": "^9.2.1",
    "pngjs": "^6.0.0",
    "prettier": "^2.5.1",
    "recast": "^0.21.1",
    "stream-buffers": "^3.0.2",
    "strip-ansi": "^6.0.1",
    "strip-indent": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsd": "^0.22.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1"
  },
  "peerDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.1",
    "cypress": "^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0"
  },
  "peerDependenciesMeta": {
    "@cypress/browserify-preprocessor": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.14.0"
  }
}
